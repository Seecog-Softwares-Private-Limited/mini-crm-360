{
  "name": "bulk-whatsapp-manager-backend",
  "version": "1.0.0",
  "description": "This is a backend of bulk whatsapp messenger app using MySQL database",
  "keywords": [
    "Javascript",
    "MySQL",
    "Node",
    "Express",
    "Bulk",
    "Whatsapp"
  ],
  "homepage": "https://github.com/NileshKumar2404/Bulk-whatsapp-Manager-Backend#readme",
  "bugs": {
    "url": "https://github.com/NileshKumar2404/Bulk-whatsapp-Manager-Backend/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NileshKumar2404/Bulk-whatsapp-Manager-Backend.git"
  },
  "license": "ISC",
  "author": "Nilesh Kumar",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "dev": "cross-env DOTENV_CONFIG_PATH=./property.env nodemon -r dotenv/config src/index.js",
    "start": "cross-env DOTENV_CONFIG_PATH=./property.env nodemon -r dotenv/config src/index.js",
    "reset-db": "cross-env DOTENV_CONFIG_PATH=./property.env node -r dotenv/config --experimental-json-modules reset-database.js",
    "migrate:campaign-email": "cross-env DOTENV_CONFIG_PATH=./property.env node -r dotenv/config src/db/migrations/add-campaign-email-support.js",
    "migrate:social-login": "cross-env DOTENV_CONFIG_PATH=./property.env node -r dotenv/config src/db/migrations/add-social-login-columns.js",
    "migrate:password-reset": "cross-env DOTENV_CONFIG_PATH=./property.env node -r dotenv/config src/db/migrations/add-password-reset-fields.js",
    "migrate:plans": "cross-env DOTENV_CONFIG_PATH=./property.env node -r dotenv/config src/db/migrations/create-plans-tables.js",
    "migrate:yearly-price": "cross-env DOTENV_CONFIG_PATH=./property.env node -r dotenv/config src/db/migrations/add-yearly-price-to-plans.js",
    "migrate:tasks": "cross-env DOTENV_CONFIG_PATH=./property.env node -r dotenv/config src/db/migrations/create-tasks-table.js",
    "migrate:notes": "cross-env DOTENV_CONFIG_PATH=./property.env node -r dotenv/config src/db/migrations/create-notes-timeline-table.js",
    "migrate:reminders": "cross-env DOTENV_CONFIG_PATH=./property.env node -r dotenv/config src/db/migrations/add-birthday-anniversary-fields.js",
    "migrate:lead-forms": "cross-env DOTENV_CONFIG_PATH=./property.env node -r dotenv/config src/db/migrations/create-lead-forms-table.js",
    "migrate:enhance-lead-forms": "cross-env DOTENV_CONFIG_PATH=./property.env node -r dotenv/config src/db/migrations/enhance-lead-forms-table.js",
    "migrate:profile-fields": "cross-env DOTENV_CONFIG_PATH=./property.env node -r dotenv/config src/db/migrations/add-profile-fields.js",
    "migrate:avatar-column": "cross-env DOTENV_CONFIG_PATH=./property.env node -r dotenv/config src/db/migrations/add-avatar-column.js",
    "migrate:phone-column": "cross-env DOTENV_CONFIG_PATH=./property.env node -r dotenv/config src/db/migrations/add-phone-column.js",
    "migrate:timezone-column": "cross-env DOTENV_CONFIG_PATH=./property.env node -r dotenv/config src/db/migrations/add-timezone-column.js",
    "migrate:all-profile-columns": "cross-env DOTENV_CONFIG_PATH=./property.env node -r dotenv/config src/db/migrations/add-all-profile-columns.js",
    "migrate:menu-items": "cross-env DOTENV_CONFIG_PATH=./property.env node -r dotenv/config src/db/migrations/create-menu-items-table.js",
    "migrate:plan-menu-items": "cross-env DOTENV_CONFIG_PATH=./property.env node -r dotenv/config src/db/migrations/create-plan-menu-items-table.js",
    "migrate:activation-fields": "cross-env DOTENV_CONFIG_PATH=./property.env node -r dotenv/config src/db/migrations/add-activation-fields.js",
    "create-admin": "cross-env DOTENV_CONFIG_PATH=./property.env node -r dotenv/config src/db/migrations/create-admin-user.js"
  },
  "dependencies": {
    "axios": "^1.12.2",
    "bcrypt": "^6.0.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "express-handlebars": "^8.0.3",
    "handlebars": "^4.7.8",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "mysql2": "^3.15.2",
    "nodemailer": "^7.0.11",
    "nodemon": "^3.1.10",
    "pdfkit": "^0.17.2",
    "puppeteer": "^24.31.0",
    "razorpay": "^2.9.6",
    "sequelize": "^6.37.7",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "cross-env": "^10.1.0"
  }
